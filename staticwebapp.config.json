{
    "routes": [
        {
            "route": "/images/*",
            "headers": {
                "cache-control": "must-revalidate, max-age=15770000"
            }
        }
    ],
    "networking": {
        "forwardingGateway": {
            "allowedForwardedHosts": [
                "ones.pw",
                "nope.pw"
            ],
            "requiredHeaders": {
                "X-CDN-TOKEN": "#{CDN-TOKEN}"
            }
        }
    }
}
